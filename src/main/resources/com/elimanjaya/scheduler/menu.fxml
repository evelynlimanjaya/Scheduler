<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0">
         <children>
            <Button fx:id="customers_btn" mnemonicParsing="false" onAction="#on_customers_btn" text="Customers">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="appointments_btn" mnemonicParsing="false" onAction="#on_appointments_btn" text="Appointments">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <HBox alignment="BOTTOM_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <Button fx:id="log_out_btn" mnemonicParsing="false" onAction="#on_log_out_btn" text="Log Out">
                     <font>
                        <Font size="18.0" />
                     </font></Button>
                  <Button fx:id="exit_btn" mnemonicParsing="false" onAction="#on_exit_btn" text="Exit">
                     <font>
                        <Font size="18.0" />
                     </font></Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
